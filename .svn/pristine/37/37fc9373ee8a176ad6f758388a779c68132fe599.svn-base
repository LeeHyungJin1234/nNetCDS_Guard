 /* circle close button */
    .close_13 {
   	  margin-top:5px;
      box-sizing: border-box;
      -moz-box-sizing: border-box;
      display: inline-block;
      width: 13px;
      height: 13px;
      background-color: #b7b9ba;
      border-radius: 50%;
      position: relative;
      cursor: pointer;
      float:left;
    }

    .close_13:after,
    .close_13:before {
      position: absolute;
      content:"";
      background-color: #fff;
    }

    .close_13:after {
      width: 1px;
      height: 7px;
      top: 3px;
      left: 6px;
      -webkit-transform: rotate(45deg);
      -moz-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
      -o-transform: rotate(45deg);
      transform: rotate(45deg);
    }

    .close_13:before {
      width: 7px;
      height: 1px;
      top: 6px;
      left: 3px;
      -webkit-transform: rotate(45deg);
      -moz-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
      -o-transform: rotate(45deg);
      transform: rotate(45deg);
    }

    .close_13:hover {
            box-shadow: none;
    }

    .close_13:hover:after,
    .close_13:hover:before {
             background : #000;
    }


