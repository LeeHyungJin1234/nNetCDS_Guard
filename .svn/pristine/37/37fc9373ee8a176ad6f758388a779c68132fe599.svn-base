.descpopup{position:fixed; width: 100%; height: 100%;left: 0; top: 0; z-index:999;}
.descpopup .bg{width: 100%; height: 100%; background: #000;opacity: 0.6; }

.descpopup .pop-wrapper{width: 100%; height: 100%; background-color: #fff; position: absolute; left:0; top:0; margin-left: 0px; margin-top: 0px; font-family: 'NanumSquare', sans-serif;}
.descpopup .header_bar{width: 100%; height: 62px; background-color: #191f28; position: relative;}
.descpopup .header_bar h1{font-size: 18px; color:#fff; line-height: 62px; margin-left: 40px;}
.descpopup .header_bar p{position: absolute; right: 40px; top: 20px;}


.descpopup .pop-wrapper > table > tbody {
    display:block;
    height:515px;
    overflow:auto;
}
.descpopup .pop-wrapper > table > tbody tr {
    display:table;
    width:100%;
    table-layout:fixed;/* even columns width , fix width of table too*/
    border-bottom:1px solid #e0e3e7;
}
.descpopup .pop-wrapper > table > thead {
    display:table;
    width:100%;
    table-layout:fixed;/* even columns width , fix width of table too*/
    height:40px;background-color: #f7f8fa; border-bottom:1px solid #686b70;
}

.descpopup .pop-wrapper > table {
    width:98%;
    border-top:1px solid #e0e3e7; border-left:1px solid #eceef1;border-right:1px solid #eceef1;margin:10px;
}

