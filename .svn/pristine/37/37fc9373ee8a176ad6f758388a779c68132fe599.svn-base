.popup .left-content > ul li label{font-size: 14px;}
.popup .left-content > ul li p{font-size: 14px;}
.popup .left-content > ul li input{font-size: 14px; color:#010c1e; width: 295px; height: 38px; border-radius: 4px; border: 1px solid #b3b5b7; box-shadow: none !important; margin-left: 12px; padding-left:11px;}
.popup .left-content > ul li input:focus {border:1px solid #cc1b3c;}
.popup .left-content > ul li input::placeholder {color:rgba(1,12,30,1,0.45);}
.popup .left-content > ul li select{font-size: 14px; color:#010c1e; width: 308px; height: 40px; border-radius: 4px; border: 1px solid #b3b5b7; box-shadow: none !important; margin-left: 9px; padding-left:11px;}

.popup .right-content{height: 660px; float: left; padding-left: 40px; padding-top: 40px; overflow: auto;}
