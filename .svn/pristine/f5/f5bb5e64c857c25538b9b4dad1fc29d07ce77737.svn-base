/* 로딩바 */
.wrap-loading{ 
	position: fixed;
	left:0;
	right:0;
	top:0;
	bottom:0;
	z-index : 100;
	background: rgba(0,0,0,0.2); /*not in ie */
	filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr='#20000000', endColorstr='#20000000'); /* ie */
}
.wrap-loading div{
	position: fixed;
	top:50%;
	left:50%;
	margin-left: -21px;
	margin-top: -21px;
}
.display-none{
	display:none;
}