/*toggle switch*/
.switch {position: relative;display: inline-block;width: 42px;height: 24px;}
.switch input {opacity: 0;width: 0;height: 0;}
.slider {position: absolute;cursor: pointer;top: 0;left: 0;right: 0;bottom: 0;background-color: #ccc;-webkit-transition: .4s;transition: .4s;border-radius: 34px;}
.slider:before {position: absolute;content: "";height: 20px;width: 20px;left: 2px;bottom: 2px;background-color: white;-webkit-transition: .4s;transition: .4s;border-radius: 50%;}
.switch input:checked + .slider {background-color: #cc1b3c;background-image: linear-gradient(#d92648, #cc1b3c, #b91130);}
.switch input:focus + .slider {box-shadow: 0 0 1px #cc1b3c;}
.switch input:checked + .slider:before {-webkit-transform: translateX(18px);-ms-transform: translateX(18px);transform: translateX(18px);}

