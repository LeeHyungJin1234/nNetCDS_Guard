/*jquery ui-picker start*/
#ui-datepicker-div .ui-datepicker-title{font-family: 'NanumGothic', sans-serif;font-size:17px;font-weight:700;}
#ui-datepicker-div .ui-datepicker-prev{margin-top:2px;left:4px;background:url("../img/date-left.png") no-repeat 50% 50%;z-index:9999;}
#ui-datepicker-div .ui-datepicker-next{margin-top:2px;right:4px;background:url("../img/date-right.png") no-repeat 50% 50%;z-index:9999;}
#ui-datepicker-div .ui-state-hover{top:2px;}
#ui-datepicker-div .ui-datepicker-prev.ui-state-hover{background:url("../img/date-left-h.png") no-repeat 50% 50%;}
#ui-datepicker-div .ui-datepicker-next.ui-state-hover{background:url("../img/date-right-h.png") no-repeat 50% 50%;}
#ui-datepicker-div .ui-datepicker-calendar thead tr span{font-family: 'NanumGothic', sans-serif;font-size:14px;font-weight:400;}
#ui-datepicker-div .ui-datepicker-calendar tbody tr a {text-align:center;padding:6px 2px;font-family: 'NanumGothic', sans-serif;font-size:14px;font-weight:400;}
#ui-datepicker-div .ui-datepicker-today .ui-state-highlight {border:0;background:#cc1b3c;border-radius:4px;color:#fff;}
#ui-datepicker-div .ui-state-hover{border:0;background:rgba(1,12,30,1,0.45);border-radius:4px;}


