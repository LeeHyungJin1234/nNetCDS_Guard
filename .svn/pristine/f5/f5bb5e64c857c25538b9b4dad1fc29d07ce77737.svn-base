@charset "utf-8";

/* ===================================
    Reset
====================================== */
body{font-family: 'NanumGothic', 'NanumSquare', 'Nunito Sans' , Gulim, Dotum; -webkit-font-smoothing: antialiased;  font-smoothing: antialiased; font-size:14px; color:#6f6f6f; font-weight: 400;line-height: 24px;}

a {font-size:15px; color:rgba(255,255,255,0.6); text-decoration: none; }
a:hover, a:active{text-decoration: none;}
a:focus, a:active, button:focus, button:active,.btn.active.focus, .btn.active:focus, .btn.focus, .btn:active.focus, .btn:active:focus, .btn:focus {outline: none;}

.opacity2 {opacity:.2;transition:0.3s;}
.opacity4 {opacity:.4;transition:0.3s;}
.display-block {display:block !important}
/* ===================================
    Typography
====================================== */
/* font family */
.point-font {font-family:'Nunito Sans', sans-serif;}
.head-font {font-family: 'NanumSquare', "NanumGothic", sans-serif;}
.main-font {font-family: 'NanumGothic', sans-serif;}
/* heading */
h1, h2, h3, h4, h5, h6 {margin:0 0 6px; padding:0; letter-spacing: 0; font-weight: 400;}
h1 {font-size:54px; line-height: 80px;}
h2 {font-size:55px; line-height:60px}
h3 {font-size:22px; line-height:34px}
h4 {font-size:40px; line-height:46px}
h5 {font-size:15px; line-height:28px}
h6 {font-size:15px; line-height:31px}
/* text size */
.text-small {font-size:13px; line-height:20px; font-weight: 400;}
.text-medium {font-size:14px; line-height:20px; font-weight: 400;}
.text-medium2 {font-size:16px; line-height:28px; font-weight: 700;}
.text-large {font-family:"NanumSquare";font-size:16px; line-height:28px;font-weight: 700;}
.title-mid {font-size: 50px; line-height:45px}
.title-large {font-size: 70px; line-height:65px}
.title-slider {font-size: 100px; line-height:95px}
.title-extra-large {font-size: 120px; line-height:100px}
/* text color */
.text-white {color:#FFF}
.text-top-color {color:rgba(255,255,255,0.6);}
.text-top-color2 {color:rgba(255,255,255,0.7);}
.text-top-color3 {color:rgba(255,255,255,0.3);}
.text-black {color:#000}
.text-gray {color:#191f28}
.text-left-color {color:rgba(25,31,40,0.6) !important;}
.text-main-color{color:rgba(1,12,30,1);}
.text-main-color2{color:rgba(1,12,30,0.65) !important;}
.text-inactive-color{color:rgba(1,12,30,0.45);}
.text-point-color{color:#cc1b3c !important;}

/* icon size */
.icon{margin-right:8px;}
.font-weight-900 {font-family:"NanumSquare", "NanumGothic" !important;font-weight: 700;}
/* ===================================
   Custom
====================================== */
/* pagination */
.pagination {border-radius: 0; padding: 0; margin: 0}
.pagination ul {display: inline-block; *display: inline; *zoom: 1; margin-left: 0; margin-bottom: 0; padding: 0; }
.pagination li {display: inline; }
.pagination a { font-family:"NanumGothic";float: left; padding: 0 16px; line-height: 40px; text-decoration: none; border: 1px solid #c7cacd; border-left-width: 0; background: #fff }
.pagination a:hover {background-color:rgba(0,0,0,.05); }
.pagination a.active  {border-bottom:2px solid #cc1b3c; color:#cc1b3c;cursor: default;font-weight:900;background-color:#fff;}
.pagination .disabled span, .pagination .disabled a, .pagination .disabled a:hover { color: #999999; background-color: transparent; cursor: default; }
.pagination li:first-child a { border-left-width: 1px; }
.pagination-centered { text-align: center; }
.pagination-right {text-align: right;}

[data-pagination],[data-pagination] *,[data-pagination] *:before,[data-pagination] *:after {-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;text-rendering: optimizeLegibility;-webkit-font-smoothing: antialiased;-moz-osx-font-smoothing: grayscale;font-kerning: auto;}
[data-pagination] {font-size: 14px;line-height: 1;font-weight: 400;-webkit-text-size-adjust: 100%;margin: 28px auto 40px;text-align: center;transition: font-size .2s ease-in-out;}
[data-pagination] ul {display: inline;font-size: 100%;margin: 0;padding: .5em;}
[data-pagination] ul li {display: inline-block;width: auto;border: 1px solid #c7cacd; margin-left:-4px;}
[data-pagination] ul li {border-left-width:0;}
[data-pagination] ul li:first-child{margin-left:0;border-left-width:1px;}
[data-pagination] ul li a {font-size: 100%;line-height: 40px; padding: 16px 16px;cursor:pointer;}
[data-pagination] .page-box {display: inline-block;font-size: 100%;padding: 0 16px;line-height: 40px; border: 1px solid #c7cacd;}
[data-pagination] > a {font-size: 140%;}
[data-pagination] a {color: #040911;font-size: 14px;font-weight:700;}
[data-pagination] ul li:hover, [data-pagination] .page-box:hover {background-color:rgba(0,0,0,.05);}
[data-pagination] li.active a {color:#cc1b3c; font-weight: 700;}
[data-pagination] li.active {border-bottom:2px solid #cc1b3c; margin-bottom:-2px;}

